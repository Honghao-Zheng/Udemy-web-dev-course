<%- include("header") -%>

    <div class="box" id="heading">
      <h1><%= ofDay %></h1>
    </div>

     <div class="box">

         <% for (var i = 0; i < event.length; i++) { %>
           <div class="item">
             <input type="checkbox">
             <p><%= event[i] %> </p>
           </div>
         <% } %>

       <form action="/" method="post">
         <input type="text" name="thing" placeholder="New item" autocomplete="off">
         <button type="submit" name="list" value=<%= ofDay %> >+</button>
       </form>
     </div>
<%- include("footer") -%>
